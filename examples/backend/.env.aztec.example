# Aztec Provider Configuration Example

# Chain configuration
AZTEC_CHAIN_ID=11155111
AZTEC_NETWORK_TYPE=testnet

# RPC endpoint (Aztec node)
AZTEC_RPC_URL=https://api.aztec.network/aztec-connect-testnet/falafel

# PXE configuration (Private eXecution Environment)
AZTEC_PXE_HOST=127.0.0.1
AZTEC_PXE_PORT=8080
AZTEC_USE_EXISTING_PXE=false
AZTEC_DATA_DIR=./.aztec-data

# Security note: Never commit actual private keys or mnemonics to version control
# AZTEC_PRIVATE_KEY=your_private_key_here
# AZTEC_MNEMONIC=your_mnemonic_here
